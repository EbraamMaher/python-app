pipeline {

    agent {label 'agent'}

    stages{
        stage('Build') {
          steps {
            script {
              withCredentials([usernamePassword(credentialsId: 'docker', usernameVariable: 'USERNAME', passwordVariable: 'PASSWORD')]) { 

              sh """
                docker ps
                """
                    }
                        } 
                }
            }
    

        stage('Deploy') {
          steps{
            script {
                 

                            sh """
                      
                              kubectl get pods -A
                            """
              

              }
            }
          }
        }

}
        
